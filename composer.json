{
    "name": "tweakers/symfony-service-mock",
    "description": "Proxy layer to allow services to have their internals replaced with a test double, without affecting a service-container's (i.e. Symfony) bindings.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Tweakers\\Test\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tweakers\\Test\\": "test/"
        }
    },
    "require": {
        "php": "^8.0",
        "friendsofphp/proxy-manager-lts": "^1.0.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Arjen van der Meijden",
            "homepage": "https://tweakers.net/"
        }
    ]
}
